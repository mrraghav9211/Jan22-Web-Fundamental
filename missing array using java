public class MyClass {
    public static void main(String args[]) {
      int []a={5,9,1,3,6,7,8,4,10};
      int total_no= a.length+1;
      int total_sum= total_no*(total_no+1)/2;
      

      //System.out.println(total_sum);
      int sum=0;
      for(int i=0; i<a.length; i++){
          sum=sum+a[i];
      }
      System.out.println("the missing no is:"+(total_sum-sum));
    }
}
